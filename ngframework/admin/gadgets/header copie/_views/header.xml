<document xmlns="http://www.w3.org/1999/xhtml" xmlns:ng="http://www.gigan.fr/" lang="fr" xml:lang="fr">  
	<div class="header">
		<ng:if value="{$typeHeader=='normal'}">
			<div style="padding-left: 50px;">
				<ul>
					<li class="menuspeleft {($active==1)?'active':''}"><a href="/programmes">Nos programmes neufs</a></li>
                    <li class="menuspe {($active==5)?'active':''}"><a href="/qui">qui sommes nous</a></li>
                    <li class="menusperight {($active==6)?'active':''}"><a data-toggle="modal" data-target="#myModalContact" href="/contact">contact</a></li>
				</ul>
			</div>
<div id='logo'>
			<a href="/"><img src="/LD/IM/gadget/header/logo.png"/></a>
		</div>
			<ng:else/>
			<div style="text-align: center;">
				<ul class="clientmenu">
